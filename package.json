{
  "name": "cordova-react-starterkit",
  "version": "0.0.1",
  "description": "Cordova App Starter Kit with React and Redux",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test NODE_PATH=. mocha --compilers js:babel-core/register --require test/setup --recursive",
    "start": "webpack-dev-server --content-base www/",
    "build": "webpack",
    "build:watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drudv/cordova-react-starterkit.git"
  },
  "author": "Dmitry Druganov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drudv/cordova-react-starterkit/issues"
  },
  "homepage": "https://github.com/drudv/cordova-react-starterkit#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.1",
    "cross-env": "^5.0.4",
    "eslint": "^4.4.0",
    "eslint-plugin-react": "^7.1.0",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "cordova-android": "^6.2.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  }
}
